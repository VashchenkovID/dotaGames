.container
  margin 24px
  color #DBE4ED
  background: rgba(63,81,181,0.3)
  padding 24px
  border-radius 10px
  box-shadow: 0px 8px 16px rgba(16, 0, 37, 0.15);
  height calc(100vh - 150px)
  margin-top 24px
  &__body
    display grid
    grid-template-columns 1fr 1fr
    gap 24px
    &__column
      display flex
      flex-direction column
      gap 16px
