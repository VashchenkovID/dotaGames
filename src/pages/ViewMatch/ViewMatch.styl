.container
  color #DBE4ED
  margin 0 auto
  max-width 1360px
  background: rgba(63,81,181,0.3)
  padding 24px
  border-radius 10px
  box-shadow: 0px 8px 16px rgba(16, 0, 37, 0.15);
  height calc(100vh - 150px)
  margin-top 24px
  align-items center
  display flex
  flex-direction column
  gap 24px
.downloadBtn
  display flex
  justify-content center
  align-items center
.download
  color:#FFFFFF
.link
  text-decoration none