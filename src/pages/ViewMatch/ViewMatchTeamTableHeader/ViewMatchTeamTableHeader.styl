.allContainer
  display flex
  flex-direction column
  gap 8px

.container
  display flex
  justify-content flex-start
  gap 24px
  align-items center
.team
  display flex
  gap 8px
  align-items center
.img
  width 60px
.subContainer
  display grid
  grid-template-columns 200px repeat(6,5vw) 0.4fr repeat(3,5vw) 1fr
  gap 8px
  align-items center
  &__cell
    text-transform uppercase
    font-weight: 600;
    font-size: 16px;
    line-height: 19px;
    white-space nowrap